<h3>SELECT A DEV PERSON</h3>
<div class="col-xs-12">
{{#each model as |dev|}}
  {{#link-to 'user' dev.login}}
		<div class="index-card col-xs-12 col-sm-3">
		  <div class="col-xs-5"><img src="{{dev.avatar_url}}"/></div>
		  <div class="col-xs-7">{{dev.login}}</div>
		</div>
	{{/link-to}}
{{/each}}
</div>
